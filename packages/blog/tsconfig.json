{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@sun-world/icons-vue": ["../icons-vue/src"],
      "@/*": ["src/*"] // 配置路径别名，用于导入 @ 符号开头的模块
    }
  },
  "include": ["src"]
}
