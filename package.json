{
  "name": "sun-world",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:icons dev:editor dev:blog  ",
    "dev:blog": "pnpm -F @sun-world/blog dev",
    "dev:icons": "pnpm -F @sun-world/icons dev",
    "watch:editor": "pnpm -F @sun-world/editor run watch",
    "build": "pnpm build:editor && pnpm build:icons && pnpm build:blog",
    "build:editor": "NODE_OPTIONS='--max-old-space-size=4096' pnpm -F @sun-world/editor build",
    "build:blog": "NODE_OPTIONS='--max-old-space-size=4096' pnpm -F @sun-world/blog run build",
    "build:icons": "NODE_OPTIONS='--max-old-space-size=2048' pnpm -F @sun-world/icons run build"
  },
  "devDependencies": {
    "@langchain/core": "^0.2.36",
    "@langchain/openai": "^0.2.11",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.17.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.5.13",
    "axios": "^1.7.9",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.1",
    "minimist": "^1.2.8",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^6.0.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "unplugin-dts": "1.0.0-beta.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.13",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "artplayer": "^5.3.0",
    "artplayer-plugin-danmuku": "^5.2.0",
    "artplayer-plugin-document-pip": "^1.0.2",
    "artplayer-plugin-hls-control": "^1.0.1",
    "hls.js": "^1.6.15",
    "jszip": "^3.10.1",
    "langchain": "^0.2.20",
    "moment": "^2.30.1",
    "openai": "^4.77.0",
    "pinia": "^3.0.3",
    "uuid": "^13.0.0",
    "vditor": "^3.10.8"
  }
}
